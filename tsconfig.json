{
    "compilerOptions": {
        "allowJs": false,
        "baseUrl": ".",
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "lib": ["DOM", "ESNext", "ESNext.AsyncIterable"],
        "module": "ESNext",
        "moduleResolution": "Node",
        "noEmit": true,
        "paths": {
            "@/*": ["./*"]
        },
        "sourceMap": true,
        "strict": true,
        "target": "ESNext",
        "types": ["@types/node", "@nuxt/types", "@nuxtjs/vuetify"]
    },
    "exclude": [".nuxt", "dist", "node_modules"]
}
