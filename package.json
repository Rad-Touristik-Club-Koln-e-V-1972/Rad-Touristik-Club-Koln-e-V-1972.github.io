{
    "name": "rad-touristik-club-koln-e-v-1972.github.io",
    "version": "1.0.0",
    "private": true,
    "packageManager": "pnpm@8.9.0",
    "TODO": "TODO WORKAROUND dependency '@yeger/vue2-masonry-wall' is needed until vuetify 3.2.0 delivers native support. See https://github.com/vuetifyjs/vuetify/issues/11177",
    "scripts": {
        "build": "nuxi generate",
        "dev": "nuxi dev",
        "lint": "pnpm lint:markup && pnpm lint:js && pnpm lint:style && pnpm lint:prettier",
        "lint:js": "eslint --cache --cache-location node_modules/.cache/.eslintcache --ext \".js,.ts,.vue\" --fix --ignore-path .gitignore --ignore-pattern pnpm-lock.yaml .",
        "lint:markup": "vue-tsc",
        "lint:prettier": "prettier --cache --cache-location node_modules/.cache/.prettiercache --ignore-path .gitignore --ignore-pattern pnpm-lock.yaml --list-different --write .",
        "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --cache --cache-location node_modules/.cache/.stylelintcache --ignore-path .gitignore --ignore-pattern pnpm-lock.yaml --fix",
        "predev": "pnpm lint",
        "preinstall": "npx only-allow pnpm",
        "prepare": "husky install && nuxi prepare"
    },
    "dependencies": {
        "@yeger/vue2-masonry-wall": "^5.0.2",
        "date-holidays": "^3.22.1",
        "maz-ui": "^2.4.7",
        "pinia": "^2.1.6",
        "postleitzahlen": "^1.0.0",
        "vue-html2pdf": "^1.8.0",
        "vue-signature-pad": "^2.0.5"
    },
    "devDependencies": {
        "@babel/core": "^7.23.0",
        "@commitlint/cli": "^17.7.2",
        "@commitlint/config-conventional": "^17.7.0",
        "@mdi/js": "^7.3.67",
        "@nuxt/bridge": "npm:@nuxt/bridge-edge@^3.0.0-rc.1-28271265.8fa71e1",
        "@nuxt/types": "^2.17.1",
        "@nuxtjs/eslint-config-typescript": "^12.1.0",
        "@nuxtjs/eslint-module": "^4.1.0",
        "@nuxtjs/router": "^1.7.0",
        "@nuxtjs/stylelint-module": "^5.1.0",
        "@nuxtjs/vuetify": "^1.12.3",
        "@pinia/nuxt": "^0.4.11",
        "@rushstack/eslint-patch": "^1.5.1",
        "@types/mime-types": "^2.1.2",
        "@types/node": "^20.8.3",
        "@vue/eslint-config-prettier": "^7.1.0",
        "babel-plugin-component": "^1.1.1",
        "buffer": "^6.0.3",
        "commitlint": "^17.7.2",
        "consola": "^3.2.3",
        "eslint": "^8.51.0",
        "eslint-plugin-nuxt": "^4.0.0",
        "eslint-plugin-vue": "^9.17.0",
        "eslint-plugin-vuetify": "^1.1.0",
        "husky": "^8.0.3",
        "lint-staged": "^14.0.1",
        "nuxi": "^3.8.4",
        "nuxt": "^2.17.1",
        "postcss-html": "^1.5.0",
        "prettier": "^3.0.3",
        "stylelint": "^15.10.3",
        "stylelint-config-recommended-vue": "^1.5.0",
        "stylelint-config-standard": "^34.0.0",
        "typescript": "^5.2.2",
        "vue": "^2.7.14",
        "vue-tsc": "^1.8.18",
        "vuetify": "^2.7.1",
        "webpack": "^4.47.0"
    },
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.vue": "bash -c vue-tsc",
        "*.{js,ts,vue}": "eslint --fix",
        "*.{css,scss,sass,html,vue}": "stylelint --fix",
        "*.**": "prettier --ignore-unknown --write"
    }
}
