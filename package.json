    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.7.0",
    "@vue/language-server": "3.2.4",
    "autoprefixer": "10.4.24",
    "commitlint": "20.4.1",
    "eslint": "9.39.2",
    "eslint-plugin-vue": "10.8.0",
    "globals": "17.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
