{
    "name": "rad-touristik-club-koln-e-v-1972.github.io",
    "version": "1.0.0",
    "private": true,
    "packageManager": "pnpm@8.4.0",
    "TODO": "TODO WORKAROUND dependency '@yeger/vue2-masonry-wall' is needed until vuetify 3.2.0 delivers native support. See https://github.com/vuetifyjs/vuetify/issues/11177",
    "scripts": {
        "build": "nuxi generate",
        "dev": "nuxi dev",
        "lint": "pnpm lint:markup && pnpm lint:js && pnpm lint:style && pnpm lint:prettier",
        "lint:js": "eslint --cache --cache-location node_modules/.cache/.eslintcache --ext \".js,.ts,.vue\" --fix --ignore-path .gitignore --ignore-pattern pnpm-lock.yaml .",
        "lint:markup": "vue-tsc",
        "lint:prettier": "prettier --cache --cache-location node_modules/.cache/.prettiercache --ignore-path .gitignore --ignore-pattern pnpm-lock.yaml --list-different --write .",
        "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --cache --cache-location node_modules/.cache/.stylelintcache --ignore-path .gitignore --ignore-pattern pnpm-lock.yaml --fix",
        "predev": "pnpm lint",
        "preinstall": "npx only-allow pnpm",
        "prepare": "husky install && nuxi prepare"
    },
    "dependencies": {
        "@yeger/vue2-masonry-wall": "~2.3.3",
        "date-holidays": "~3.19.0",
        "pinia": "~2.0.35"
    },
    "devDependencies": {
        "@babel/core": "~7.21.8",
        "@commitlint/cli": "~17.6.3",
        "@commitlint/config-conventional": "~17.6.3",
        "@mdi/js": "~7.2.96",
        "@nuxt/bridge": "npm:@nuxt/bridge-edge@~3.0.0-28054996.ab449e5",
        "@nuxt/types": "~2.16.3",
        "@nuxtjs/eslint-config-typescript": "~12.0.0",
        "@nuxtjs/eslint-module": "~4.0.2",
        "@nuxtjs/router": "~1.7.0",
        "@nuxtjs/stylelint-module": "~5.0.1",
        "@nuxtjs/vuetify": "~1.12.3",
        "@pinia/nuxt": "~0.4.9",
        "@rushstack/eslint-patch": "~1.2.0",
        "@types/mime-types": "~2.1.1",
        "@types/node": "~18.16.4",
        "@vue/eslint-config-prettier": "~7.1.0",
        "@vue/eslint-config-typescript": "~11.0.2",
        "buffer": "~6.0.3",
        "commitlint": "~17.6.3",
        "consola": "~3.1.0",
        "eslint": "~8.39.0",
        "eslint-plugin-nuxt": "~4.0.0",
        "eslint-plugin-vue": "~9.11.0",
        "eslint-plugin-vuetify": "~1.1.0",
        "husky": "~8.0.3",
        "lint-staged": "~13.2.2",
        "nuxt": "~2.16.3",
        "postcss-html": "~1.5.0",
        "prettier": "~2.8.8",
        "stylelint": "~15.6.1",
        "stylelint-config-recommended-vue": "~1.4.0",
        "stylelint-config-standard": "~33.0.0",
        "typescript": "~5.0.4",
        "vue": "~2.7.14",
        "vue-tsc": "~1.6.4",
        "vuetify": "~2.6.15",
        "webpack": "~4.46.0"
    },
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.vue": "bash -c vue-tsc",
        "*.{js,ts,vue}": "eslint --fix",
        "*.{css,scss,sass,html,vue}": "stylelint --fix",
        "*.**": "prettier --ignore-unknown --write"
    }
}
