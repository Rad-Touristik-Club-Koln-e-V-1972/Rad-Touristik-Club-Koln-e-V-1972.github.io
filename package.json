    "@vue/language-server": "3.0.3",
    "autoprefixer": "10.4.21",
    "commitlint": "19.8.1",
    "eslint": "9.31.0",
    "eslint-plugin-vue": "10.3.0",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "prettier": "3.6.2",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "vite-plugin-checker": "0.10.0",
    "vite-plugin-sitemap": "0.8.2",
    "vue-tsc": "3.0.2"
