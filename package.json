{
    "name": "rad-touristik-club-koln-e-v-1972.github.io",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "nuxt",
        "generate": "nuxt generate",
        "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore --fix .",
        "lint:prettier": "prettier --write --list-different .",
        "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore --fix",
        "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
        "prepare": "husky install"
    },
    "lint-staged": {
        "*.{js,ts,vue}": "eslint --fix",
        "*.{css,scss,sass,html,vue}": "stylelint --fix",
        "*.**": "prettier --ignore-unknown --write"
    },
    "dependencies": {
        "@nuxtjs/composition-api": "^0.32.0",
        "@pinia/nuxt": "^0.1.9",
        "@rushstack/eslint-patch": "^1.1.4",
        "@types/marked": "^4.0.3",
        "core-js": "^3.23.3",
        "date-holidays": "^3.16.2",
        "marked": "^4.0.17",
        "nuxt": "^2.15.8",
        "vue": "^2.6.14",
        "vuetify": "^2.6.7"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.0.3",
        "@commitlint/config-conventional": "^17.0.3",
        "@mdi/js": "^6.9.96",
        "@nuxt/types": "^2.15.8",
        "@nuxt/typescript-build": "^2.1.0",
        "@nuxtjs/eslint-config-typescript": "^10.0.0",
        "@nuxtjs/eslint-module": "^3.1.0",
        "@nuxtjs/stylelint-module": "^4.1.0",
        "@nuxtjs/vuetify": "^1.12.3",
        "@vue/eslint-config-prettier": "^7.0.0",
        "eslint": "^8.19.0",
        "eslint-plugin-nuxt": "^3.2.0",
        "eslint-plugin-vue": "^9.2.0",
        "eslint-plugin-vuetify": "^1.1.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "postcss-html": "^1.4.1",
        "prettier": "^2.7.1",
        "stylelint": "^14.9.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recommended-vue": "^1.4.0",
        "stylelint-config-standard": "^26.0.0"
    }
}
