    "german-metadata": "2.0.1",
    "jspdf": "3.0.3",
    "libphonenumber-js": "1.12.25",
    "maz-ui": "4.2.1",
    "mime": "4.1.0",
    "pinia": "3.0.3",
    "quasar": "2.18.5",
