    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/language-server": "3.1.2",
    "autoprefixer": "10.4.21",
    "commitlint": "20.1.0",
    "eslint": "9.39.0",
    "eslint-plugin-vue": "10.5.1",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "prettier": "3.6.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "vite-plugin-checker": "0.11.0",
    "vite-plugin-sitemap": "0.8.2",
    "vue-tsc": "3.1.2"
  },
  "lint-staged": {
    "*.**": [
