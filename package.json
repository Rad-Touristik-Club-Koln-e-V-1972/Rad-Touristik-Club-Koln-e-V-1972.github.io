  "lint-staged": {
    "*.**": [
      "eslint --fix",
      "prettier --ignore-unknown --write"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "core-js",
      "esbuild",
      "vue-demi"
    ]
