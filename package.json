    "date-holidays": "3.24.4",
    "german-metadata": "2.0.1",
    "jspdf": "3.0.1",
    "libphonenumber-js": "1.12.10",
    "maz-ui": "3.50.1",
    "mime": "4.0.7",
    "pinia": "3.0.3",
    "quasar": "2.18.1",
    "signature_pad": "5.0.10",
